<script>
  var slideTotal = jQuery('.slides .slide').length,
    slidesWidth = jQuery('.slides .slide').width(),
    maxMargin = -slideTotal * slidesWidth + slidesWidth,
    pos = 0,
    curSlide = 0,
    autoOff = false;

jQuery(function(){
  jQuery('.slidesContainer').width(slidesWidth * slideTotal);
  
  jQuery('.slides .slide').each(function(){
    
  });
  
  slideLoop();
  
});

function slideLoop(){
  if(autoOff === false){
    pos = -slidesWidth * curSlide + slidesWidth;
    if(pos <= maxMargin){
      pos = 0;
      curSlide=1;
    } else {
      pos = pos - slidesWidth; 
      curSlide=1;
    }
    
    
    jQuery(document ).ready(function() {
    jQuery('.slidesContainer').delay(1000).animate({ marginLeft: pos + 'px' }, 600, function(){
      setTimeout(function(){ slideLoop() }, 800);
    });
    });  
    
  }
}

</script>


